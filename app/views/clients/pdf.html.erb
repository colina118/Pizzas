
<div class="col-lg-12">
  <table class="table">
    <thead>
    <tr>
      <th class="table-header">Nombre</th>
      <th class="table-header"> Apellido</th>
      <th class="table-header"> Telefono</th>
      <th class="table-header"> Direccion</th>


      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @clients.each do |client| %>
        <tr>
          <td class="table-cell"><%= client.name %> </td>
          <td class="table-cell"><%= client.lastname %></td>
          <td class="table-cell"><%= client.phone_number %></td>
          <td class="table-cell"><%= client.address %></td>

        </tr>
    <% end %>
    </tbody>
  </table>
</div>